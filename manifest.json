{
  "manifest_version": 3,
  "name": "github-history",
  "version": "1.0",
  // "icons": {
  //   "16": "images/icon-16.png",
  //   "32": "images/icon-32.png",
  //   "48": "images/icon-48.png",
  //   "128": "images/icon-128.png"
  // },
  "permissions": [
    "storage",
    "background",
    "history"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "dist/history-background.mjs"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "dist/history-modal.css"
      ],
      "js": [
        "dist/history.mjs"
      ]
    }
  ]
}